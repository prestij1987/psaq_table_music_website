

 # Созадаем таблицу исполнителя
CREATE TABLE IF NOT EXISTS singers (Id serial primary key, 
First_name varchar(40) not null, Last_name varchar(50)); 



# Создаем таблицу альбомов
CREATE TABLE IF NOT EXISTS album (Id serial primary key, 
Name_album varchar(40) not null, Year date);


# Создаем таблицу музыкальных треков
CREATE TABLE music_track (Id serial primary key, 
Name_sing text not null, Time_play timestamp);  


# Создание таблцы музыкальный сайт
CREATE TABLE IF NOT EXISTS music_website (Id serial primary key, 
List_singers_id  varchar(40) unique not null, List_album_singers_id varchar(50) unique not null, 
List_track_id  text unique not null);

# Cоздание внешнего ключа singers и music_website
ALTER TABLE singers ADD singers_id BIGINT REFERENCES music_website (list_singers);

# Создание внешнего ключа album c music_website
ALTER TABLE album ADD album_id BIGINT REFERENCES music_website (list_album);   

# Cоздание внешнего ключа music_track b music_website
ALTER TABLE music_track ADD music_track_id BIGINT REFERENCES music_website (list_track; 
